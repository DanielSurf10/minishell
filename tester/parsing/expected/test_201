((echo hi | cat -e) | cat -e)
TYPE: SUBSHELL
SUBSHELL:
	TYPE: PIPE
	LEFT:
		TYPE: SUBSHELL
		SUBSHELL:
			TYPE: PIPE
			LEFT:
				TYPE: COMMAND:
					 1 - WORD = echo
					 2 - WORD = hi
			RIGHT:
				TYPE: COMMAND:
					 1 - WORD = cat
					 2 - WORD = -e
	RIGHT:
		TYPE: COMMAND:
			 1 - WORD = cat
			 2 - WORD = -e
