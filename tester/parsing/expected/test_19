ls -l | cat -e && echo deu bom || echo deu ruim agora
TYPE: OR
LEFT:
	TYPE: AND
	LEFT:
		TYPE: PIPE
		LEFT:
			TYPE: COMMAND:
				 1 - WORD = ls
				 2 - WORD = -l
		RIGHT:
			TYPE: COMMAND:
				 1 - WORD = cat
				 2 - WORD = -e
	RIGHT:
		TYPE: COMMAND:
			 1 - WORD = echo
			 2 - WORD = deu
			 3 - WORD = bom
RIGHT:
	TYPE: COMMAND:
		 1 - WORD = echo
		 2 - WORD = deu
		 3 - WORD = ruim
		 4 - WORD = agora
