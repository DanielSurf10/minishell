( /bin/ls < infile ) < infile | < infile /bin/cat -e && ( ( ( < infile /bin/cat -e ) ) )
TYPE: AND
LEFT:
	TYPE: PIPE
	LEFT:
		TYPE: REDIRECT_INPUT
		LEFT:
			TYPE: COMMAND:
				 1 - WORD = infile
		RIGHT:
			TYPE: SUBSHELL
			SUBSHELL:
				TYPE: REDIRECT_INPUT
				LEFT:
					TYPE: COMMAND:
						 1 - WORD = infile
				RIGHT:
					TYPE: COMMAND:
						 1 - WORD = /bin/ls
	RIGHT:
		TYPE: REDIRECT_INPUT
		LEFT:
			TYPE: COMMAND:
				 1 - WORD = infile
		RIGHT:
			TYPE: COMMAND:
				 1 - WORD = /bin/cat
				 2 - WORD = -e
RIGHT:
	TYPE: SUBSHELL
	SUBSHELL:
		TYPE: SUBSHELL
		SUBSHELL:
			TYPE: SUBSHELL
			SUBSHELL:
				TYPE: REDIRECT_INPUT
				LEFT:
					TYPE: COMMAND:
						 1 - WORD = infile
				RIGHT:
					TYPE: COMMAND:
						 1 - WORD = /bin/cat
						 2 - WORD = -e
