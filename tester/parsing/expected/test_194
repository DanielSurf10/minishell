echo hi | (echo hello && echo bye) | cat -e
TYPE: PIPE
LEFT:
	TYPE: PIPE
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = echo
			 2 - WORD = hi
	RIGHT:
		TYPE: SUBSHELL
		SUBSHELL:
			TYPE: AND
			LEFT:
				TYPE: COMMAND:
					 1 - WORD = echo
					 2 - WORD = hello
			RIGHT:
				TYPE: COMMAND:
					 1 - WORD = echo
					 2 - WORD = bye
RIGHT:
	TYPE: COMMAND:
		 1 - WORD = cat
		 2 - WORD = -e
