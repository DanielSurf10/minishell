ech oi && echo hi | echo hello | echo bonjour || echo haha
TYPE: OR
LEFT:
	TYPE: AND
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = ech
			 2 - WORD = oi
	RIGHT:
		TYPE: PIPE
		LEFT:
			TYPE: PIPE
			LEFT:
				TYPE: COMMAND:
					 1 - WORD = echo
					 2 - WORD = hi
			RIGHT:
				TYPE: COMMAND:
					 1 - WORD = echo
					 2 - WORD = hello
		RIGHT:
			TYPE: COMMAND:
				 1 - WORD = echo
				 2 - WORD = bonjour
RIGHT:
	TYPE: COMMAND:
		 1 - WORD = echo
		 2 - WORD = haha
