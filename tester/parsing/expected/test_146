export carol=55 && echo $carolbia$ | cat -e > outfile3 && echo bye
TYPE: AND
LEFT:
	TYPE: AND
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = export
			 2 - WORD = carol=55
	RIGHT:
		TYPE: PIPE
		LEFT:
			TYPE: COMMAND:
				 1 - WORD = echo
				 2 - WORD = $carolbia$
		RIGHT:
			TYPE: REDIRECT_OUTPUT
			LEFT:
				TYPE: COMMAND:
					 1 - WORD = outfile3
			RIGHT:
				TYPE: COMMAND:
					 1 - WORD = cat
					 2 - WORD = -e
RIGHT:
	TYPE: COMMAND:
		 1 - WORD = echo
		 2 - WORD = bye
