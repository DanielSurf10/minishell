export var30 && export var30=carol && export | grep var30
TYPE: AND
LEFT:
	TYPE: AND
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = export
			 2 - WORD = var30
	RIGHT:
		TYPE: COMMAND:
			 1 - WORD = export
			 2 - WORD = var30=carol
RIGHT:
	TYPE: PIPE
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = export
	RIGHT:
		TYPE: COMMAND:
			 1 - WORD = grep
			 2 - WORD = var30
