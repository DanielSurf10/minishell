ech oi || echo oi && echo hi | cat -e
TYPE: AND
LEFT:
	TYPE: OR
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = ech
			 2 - WORD = oi
	RIGHT:
		TYPE: COMMAND:
			 1 - WORD = echo
			 2 - WORD = oi
RIGHT:
	TYPE: PIPE
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = echo
			 2 - WORD = hi
	RIGHT:
		TYPE: COMMAND:
			 1 - WORD = cat
			 2 - WORD = -e
