< infile cat > outfile -e && ls < infile > -a outfile -l
TYPE: AND
LEFT:
	TYPE: REDIRECT_INPUT
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = infile
	RIGHT:
		TYPE: REDIRECT_OUTPUT
		LEFT:
			TYPE: COMMAND:
				 1 - WORD = outfile
		RIGHT:
			TYPE: COMMAND:
				 1 - WORD = cat
				 2 - WORD = -e
RIGHT:
	TYPE: REDIRECT_INPUT
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = infile
	RIGHT:
		TYPE: REDIRECT_OUTPUT
		LEFT:
			TYPE: COMMAND:
				 1 - WORD = -a
		RIGHT:
			TYPE: COMMAND:
				 1 - WORD = ls
				 2 - WORD = outfile
				 3 - WORD = -l
