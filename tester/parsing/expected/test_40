< infile (/bin/cat > out -e > out2 | /bin/ps)
TYPE: REDIRECT_INPUT
LEFT:
	TYPE: COMMAND:
		 1 - WORD = infile
RIGHT:
	TYPE: SUBSHELL
	SUBSHELL:
		TYPE: PIPE
		LEFT:
			TYPE: REDIRECT_OUTPUT
			LEFT:
				TYPE: COMMAND:
					 1 - WORD = out
			RIGHT:
				TYPE: REDIRECT_OUTPUT
				LEFT:
					TYPE: COMMAND:
						 1 - WORD = out2
				RIGHT:
					TYPE: COMMAND:
						 1 - WORD = /bin/cat
						 2 - WORD = -e
		RIGHT:
			TYPE: COMMAND:
				 1 - WORD = /bin/ps
