(ech hi || echo carol > outfile1 && echo bia) | cat -e && echo bye
TYPE: AND
LEFT:
	TYPE: PIPE
	LEFT:
		TYPE: SUBSHELL
		SUBSHELL:
			TYPE: AND
			LEFT:
				TYPE: OR
				LEFT:
					TYPE: COMMAND:
						 1 - WORD = ech
						 2 - WORD = hi
				RIGHT:
					TYPE: REDIRECT_OUTPUT
					LEFT:
						TYPE: COMMAND:
							 1 - WORD = outfile1
					RIGHT:
						TYPE: COMMAND:
							 1 - WORD = echo
							 2 - WORD = carol
			RIGHT:
				TYPE: COMMAND:
					 1 - WORD = echo
					 2 - WORD = bia
	RIGHT:
		TYPE: COMMAND:
			 1 - WORD = cat
			 2 - WORD = -e
RIGHT:
	TYPE: COMMAND:
		 1 - WORD = echo
		 2 - WORD = bye
