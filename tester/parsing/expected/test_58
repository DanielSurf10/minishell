echo hello | cat -e && ls | wc -l
TYPE: AND
LEFT:
	TYPE: PIPE
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = echo
			 2 - WORD = hello
	RIGHT:
		TYPE: COMMAND:
			 1 - WORD = cat
			 2 - WORD = -e
RIGHT:
	TYPE: PIPE
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = ls
	RIGHT:
		TYPE: COMMAND:
			 1 - WORD = wc
			 2 - WORD = -l
