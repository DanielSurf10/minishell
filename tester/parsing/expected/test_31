< infile cat > outfile | < infile grep a
TYPE: PIPE
LEFT:
	TYPE: REDIRECT_INPUT
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = infile
	RIGHT:
		TYPE: REDIRECT_OUTPUT
		LEFT:
			TYPE: COMMAND:
				 1 - WORD = outfile
		RIGHT:
			TYPE: COMMAND:
				 1 - WORD = cat
RIGHT:
	TYPE: REDIRECT_INPUT
	LEFT:
		TYPE: COMMAND:
			 1 - WORD = infile
	RIGHT:
		TYPE: COMMAND:
			 1 - WORD = grep
			 2 - WORD = a
